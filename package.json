{
  "name": "y-component",
  "version": "2.0.3",
  "description": "Tools for building web components that work across popular UI frameworks.",
  "main": "es5/index.js",
  "scripts": {
    "test": "echo \"Warning: no test specified\" && exit 0",
    "mkdirs": "mkdir -p es5",
    "clean": "rm -rf es5",
    "build": "npm run mkdirs && babel src -d es5",
    "watch": "npm run mkdirs && babel -w src -d es5",
    "lint": "eslint src",
    "dev": "npm run watch",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build"
  },
  "author": "Florian Klampfer <f.klampfer@gmail.com> (http://qwtel.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "7.3.0",
    "cash": "0.8.0",
    "eslint": "3.7.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0"
  },
  "dependencies": {
    "camel-case": "3.0.0",
    "core-js": "2.4.1",
    "param-case": "2.1.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwtel/y-component.git"
  },
  "keywords": [
    "webcomponents",
    "web",
    "components",
    "vanilla",
    "component",
    "jquery",
    "y"
  ],
  "bugs": {
    "url": "https://github.com/qwtel/y-component/issues"
  },
  "homepage": "https://github.com/qwtel/y-component#readme"
}
